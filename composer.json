{
  "name": "myqee/hyperf",
  "description": "MyQEE server library for Hyperf.",
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "hyperf",
    "server",
    "myqee"
  ],
  "require": {
    "php": ">=7.2",
    "ext-swoole": ">=4.4",
    "myqee/server": "~2.1.0",
    "hyperf/di": "~1.1.0",
    "hyperf/config": "~1.1.0",
    "hyperf/command": "~1.1.0",
    "hyperf/logger": "~1.1.0",
    "hyperf/server": "~1.1.0",
    "psr/http-server-middleware": "~1.0.0"
  },
  "require-dev": {
    "myqee/server": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "MyQEE\\Hyperf\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "hyperf": {
      "config": "MyQEE\\Hyperf\\ConfigProvider"
    }
  },
  "bin": [
  ]
}